<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="taxi.tm.com.taxi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/TaxiTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CallingUpActivity"/>
        <activity android:name=".OrderInfoActivity"/>
        <activity android:name=".MessageActivity"/>
        <activity android:name=".NavigationActivity"/>
        <activity android:name=".TelephoneActivity"/>
        <activity android:name=".PTOIActivity"/>
        <activity android:name=".DriverInfoActivity"/>
        <activity android:name=".TaxiStatusActivity"/>
    </application>

</manifest>